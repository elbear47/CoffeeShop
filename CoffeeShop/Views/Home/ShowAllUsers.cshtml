@model List<User>

@{
    
    <table class="table">
        <thead>
            <tr>
                <th scope="col">User Id</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (User user in Model)
            {
            <tr>
                <td> @user.Id</td>
                <td> @user.FirstName</td>
                <td> @user.LastName</td>
                <td> @user.Email</td>

            </tr>
            }

        </tbody>
    </table>


}

